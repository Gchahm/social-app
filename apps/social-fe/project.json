{
  "name": "social-fe",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/social-fe/src",
  "projectType": "application",
  "tags": ["app", "ui"],
  "targets": {
    "synth": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/social-fe",
        "command": "cdk synth"
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "cwd": "apps/social-fe",
        "command": "cdk deploy --all --require-approval never"
      }
    },
    "generate-outputs": {
      "executor": "nx:run-commands",
      "options": {
        "command": "OUTPUT_FILE=apps/social-fe/api-stack.outputs.json .github/scripts/fetch-backend-outputs.sh"
      }
    }
  }
}
